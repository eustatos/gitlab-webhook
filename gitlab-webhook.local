<VirtualHost *:80>

        ServerName gitlab-webhook.local
	ServerAdmin webmaster@localhost
	DocumentRoot "/home/eustatos/Yandex.Disk/eclipse-workspace/gitlab-webhook/"
<Directory />
    Options Indexes FollowSymLinks Includes ExecCGI
    AllowOverride All
    Require all granted
    Allow from all
</Directory>
	ErrorLog ${APACHE_LOG_DIR}/error.log
	CustomLog ${APACHE_LOG_DIR}/access.log combined

</VirtualHost>

